package br.com.mv.dto;

import br.com.mv.entity.Genero;

public class GeneroDto {

	private String nome;
	
	public GeneroDto() {
		
	}

	public GeneroDto(String nome) {
		super();
		this.nome = nome;
	}
	
	public GeneroDto(Genero entity) {
		
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}
	
	
}
